{
  "name": "bugtracker",
  "description": "A test application that demonstrates issues",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "scripts": {
    "npm-install": "npm install",
    "pwd": "echo %cd%",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "typings": "typings install",
    "liveserver-dev": "live-server --open=static --entry-file=\"static/index.html\" --ignorePattern=\".*\\.(ts|map)\"",
    "start-dev": "concurrent \"npm run typings\" \"npm run tscw\" \"npm run liveserver-dev\""
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.14.2"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.12",
    "angular2-redux-store": "1.2.0",
    "bootstrap": "3.3.6",
    "crypto": "0.0.3",
    "es6-promise": "3.1.2",
    "es6-shim": "0.35.0",
    "jquery": "2.2.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "2.0.0",
    "live-server": "0.9.2",
    "typescript": "1.8.9",
    "typings": "0.7.9"
  }
}